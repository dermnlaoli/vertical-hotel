<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <data>    
        <record id="data_mesin_form" model="ir.ui.view">
            <field name="name">data_mesin_form</field>
            <field name="model">data.mesin.urw</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <h1>
                                <field name="no_mesin" />
                            </h1>
                        </group>
                        <group>
                            <field name="kode_kain" />
                            <field name="block_id" />
                            <field name="deret" />
                        </group>
                    </sheet> 
                </form>
            </field>
        </record>

        <record id="data_mesin_tree" model="ir.ui.view">
            <field name="name">data_mesin_tree</field>
            <field name="model">data.mesin.urw</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="no_mesin"/>
                    <field name="kode_kain"/>
                    <field name="block_id"/>
                    <field name="deret"/>
                </tree>
            </field>
        </record>    
  </data>
</odoo>
